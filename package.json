{
  "name": "vue-progress-button",
  "version": "1.0.4",
  "description": "A Vue project to make buttons that show the progress when being clicked on.",
  "author": "Richard de Jongh <richarddejongh1995@gmail.com>",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/websmurf/vue-progress-button",
  "keywords": [
    "vue",
    "vuejs",
    "vue-component",
    "progress",
    "button"
  ],
  "main": "dist/build.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "unit": "cross-env BABEL_ENV=test karma start test/karma.conf.js --single-run",
    "lint": "eslint --ext .js,.vue src",
    "preversion": "npm run unit",
    "version": "npm run build && git add -A dist",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/websmurf/vue-progress-button"
  },
  "bugs": {
    "url": "https://github.com/websmurf/vue-progress-button/issues"
  },
  "dependencies": {
    "babel-runtime": "^6.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.0.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "cross-env": "^3.2.4",
    "css-loader": "^0.25.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^0.9.0",
    "inject-loader": "^3.0.0",
    "karma": "^1.7.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-sinon-chai": "^1.3.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.3",
    "lolex": "^1.6.0",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.0",
